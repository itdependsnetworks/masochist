{
  "name": "masochist-experimental",
  "version": "0.0.1",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "main": "index.ts",
  "devDependencies": {
    "bun-types": "*",
    "prettier": "*",
    "typescript": "*"
  },
  "description": "Website infrastructure for overengineers",
  "license": "MIT",
  "private": true,
  "scripts": {
    "benchmark": "cd packages/benchmark && bun benchmark",
    "benchmark:dynamic:lexer": "cd packages/benchmark && bun benchmark:dynamic:lexer",
    "benchmark:reference:lexer": "cd packages/benchmark && bun benchmark:reference:lexer",
    "benchmark:static:lexer": "cd packages/benchmark && bun benchmark:static:lexer",
    "benchmark:dynamic:parser": "cd packages/benchmark && bun benchmark:dynamic:parser",
    "benchmark:reference:parser": "cd packages/benchmark && bun benchmark:reference:parser",
    "benchmark:table:parser": "cd packages/benchmark && bun benchmark:table:parser",
    "benchmark:static:parser": "cd packages/benchmark && bun benchmark:static:parser",
    "tsc": "tsc --build --emitDeclarationOnly",
    "tsc:clean": "tsc --build --clean",
    "tsc:dry": "tsc --build --dry",
    "tsc:force": "tsc --build --force --emitDeclarationOnly",
    "tsc:watch": "tsc --build --watch --emitDeclarationOnly",
    "format": "support/format.sh",
    "format:check": "support/check-format.sh",
    "lint": "support/lint.sh"
  },
  "workspaces": [
    "packages/*"
  ]
}
