digraph finite_state_machine {
  bgcolor = "transparent";
  rankdir = LR;
  ratio = 0.5625; // 16:9.

  // Invisible node from which to draw start transition to start state.
  node [style = invis]; -1;

  // Accept state.
  node [color = "white", fontcolor = "white", style = "", shape = doublecircle]; 8;

  // Catch-all (default) for remaining states.
  node [color = "white", fontcolor = "white", style = "", shape = circle, xlabel = ""];

  -1 -> 0 [color = "white", fontcolor = "white"];
  0 -> 1 [color = "white", fontcolor = "white", label = "f"];
  1 -> 2 [color = "white", fontcolor = "white", label = "u"];
  2 -> 3 [color = "white", fontcolor = "white", label = "n"];
  3 -> 4 [color = "white", fontcolor = "white", label = "c"];
  4 -> 5 [color = "white", fontcolor = "white", label = "t"];
  5 -> 6 [color = "white", fontcolor = "white", label = "i"];
  6 -> 7 [color = "white", fontcolor = "white", label = "o"];
  7 -> 8 [color = "white", fontcolor = "white", label = "n"];
}