digraph finite_state_machine {
  bgcolor = "transparent";
  rankdir = LR;
  ratio = 0.5625; // 16:9.

  // Invisible node from which to draw start transition to start state.
  node [style = invis]; -1;

  // Accept state.
  node [color = "black", fontcolor = "black", style = "", shape = doublecircle]; 6;

  // Catch-all (default) for remaining states.
  node [color = "black", fontcolor = "black", style = "", shape = circle, xlabel = ""];

  -1 -> 0 [color = "black", fontcolor = "black"];
  0 -> 1 [color = "black", fontcolor = "black", label = "\\\\"];
  1 -> 2 [color = "black", fontcolor = "black", label = "u"];
  2 -> 3 [color = "black", fontcolor = "black", label = "0-9"];
  2 -> 3 [color = "black", fontcolor = "black", label = "A-F"];
  2 -> 3 [color = "black", fontcolor = "black", label = "a-f"];
  3 -> 4 [color = "black", fontcolor = "black", label = "0-9"];
  3 -> 4 [color = "black", fontcolor = "black", label = "A-F"];
  3 -> 4 [color = "black", fontcolor = "black", label = "a-f"];
  4 -> 5 [color = "black", fontcolor = "black", label = "0-9"];
  4 -> 5 [color = "black", fontcolor = "black", label = "A-F"];
  4 -> 5 [color = "black", fontcolor = "black", label = "a-f"];
  5 -> 6 [color = "black", fontcolor = "black", label = "0-9"];
  5 -> 6 [color = "black", fontcolor = "black", label = "A-F"];
  5 -> 6 [color = "black", fontcolor = "black", label = "a-f"];
}