{
  "name": "@masochist/benchmark",
  "version": "0.0.1",
  "description": "Masochist benchmarks",
  "dependencies": {
    "@masochist/common": "*",
    "@masochist/graphql": "*",
    "@masochist/legacy": "*",
    "@masochist/lexer": "*",
    "@masochist/parser": "*"
  },
  "main": "lib/index.js",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "benchmark": "node lib/index.js",
    "benchmark:dynamic:lexer": "node lib/benchmark-dynamic-lexer.js",
    "benchmark:dynamic:parser": "node lib/benchmark-dynamic-parser.js",
    "benchmark:reference:lexer": "node lib/benchmark-reference-lexer.js",
    "benchmark:reference:parser": "node lib/benchmark-reference-parser.js",
    "benchmark:static:lexer": "node lib/benchmark-static-lexer.js",
    "benchmark:static:parser": "node lib/benchmark-static-parser.js",
    "benchmark:table:parser": "node lib/benchmark-table-parser.js",
    "build": "cd ../.. && yarn build",
    "format": "cd ../.. && yarn format",
    "inspect:dynamic:parser": "echo 'Go to chrome://inspect' && node --inspect-brk lib/benchmark-dynamic-parser.js",
    "profile:deopt:dynamic:lexer": "node --trace-opt --trace-deopt-verbose lib/benchmark-dynamic-lexer.js",
    "profile:deopt:dynamic:parser": "node --trace-opt --trace-deopt-verbose lib/benchmark-dynamic-parser.js",
    "profile:deopt:reference:lexer": "node --trace-opt --trace-deopt-verbose lib/benchmark-reference-lexer.js",
    "profile:deopt:static:lexer": "node --trace-opt --trace-deopt-verbose lib/benchmark-static-lexer.js",
    "profile:deopt:static:parser": "node --trace-opt --trace-deopt-verbose lib/benchmark-static-parser.js",
    "profile:dynamic:lexer": "node --trace-warnings --prof --no-logfile-per-isolate lib/benchmark-dynamic-lexer.js && node --prof-process v8.log",
    "profile:dynamic:parser": "node --prof --no-logfile-per-isolate lib/benchmark-dynamic-parser.js && node --prof-process v8.log",
    "profile:reference:lexer": "node --prof --no-logfile-per-isolate lib/benchmark-reference-lexer.js && node --prof-process v8.log",
    "profile:static:lexer": "node --trace-warnings --prof --no-logfile-per-isolate lib/benchmark-static-lexer.js && node --prof-process v8.log",
    "profile:static:parser": "node --prof --no-logfile-per-isolate lib/benchmark-static-parser.js && node --prof-process v8.log",
    "profile:table:parser": "node --prof --no-logfile-per-isolate lib/benchmark-table-parser.js && node --prof-process v8.log",
    "test": "cd ../.. && yarn test packages/benchmark"
  }
}
