{
  "name": "@masochist/benchmark",
  "version": "0.0.1",
  "description": "Masochist benchmarks",
  "dependencies": {
    "@masochist/common": "*",
    "@masochist/graphql": "*",
    "@masochist/legacy": "*",
    "@masochist/lexer": "*",
    "@masochist/parser": "*",
    "graphql": "*"
  },
  "main": "src/index.ts",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "scripts": {
    "benchmark": "bun src/index.ts",
    "benchmark:dynamic:lexer": "bun src/benchmark-dynamic-lexer.ts",
    "benchmark:dynamic:parser": "bun src/benchmark-dynamic-parser.ts",
    "benchmark:reference:lexer": "bun src/benchmark-reference-lexer.ts",
    "benchmark:reference:parser": "bun src/benchmark-reference-parser.ts",
    "benchmark:static:lexer": "bun src/benchmark-static-lexer.ts",
    "benchmark:static:parser": "bun src/benchmark-static-parser.ts",
    "benchmark:table:parser": "bun src/benchmark-table-parser.ts",
    "build": "cd ../.. && bun tsc",
    "format": "cd ../.. && bun format",
    "test": "cd ../.. && bun test packages/benchmark"
  }
}
