    "@masochist/common": "*",
    "@masochist/legacy": "*",
    "@masochist/lexer": "*"
  },
  "main": "lib/index.js",
  "author": "Greg Hurrell <greg@hurrell.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
